@using XX_PhotoPrint.Service
@using System.Web.Script.Serialization
@{
    Layout = null;

    var js = new JavaScriptSerializer();

    var provinces = SQL.Query("select ProvID,ProvName from Province order by ProvName");
    var cities = SQL.Query("select CityID,CityName,ProvID from City order by CityName");

    Output.Write(js.Serialize(new { success = true, provinces = provinces, cities = cities }));
    return;
}
